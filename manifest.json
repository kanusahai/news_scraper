{
  "name": "Web-Scraper",
  "version": "1.0",
  "description": "Scrape News!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "*://www.nytimes.com/*",
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://www.nytimes.com/*" ],
      "js": [ "contentScript.js" ]
    }
  ],
  "manifest_version": 2
}
